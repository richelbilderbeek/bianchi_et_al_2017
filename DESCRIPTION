Package: bianchietal2017
Type: Package
Title: Bianchi Et Al Two Thousand Seventeen
Version: 1.0
Authors@R: c(
  person("Johannes", "Textor", email = "johannes.textor@radboudumc.nl", role = c("aut", "cre"), comment = c(ORCID = "0000-0002-0459-9458")),
